export ZSH=$HOME/.oh-my-zsh

ZSH_THEME="jax"

DISABLE_AUTO_UPDATE="true"

COMPLETION_WAITING_DOTS="true"

plugins=(git zsh-autosuggestions)

source $ZSH/oh-my-zsh.sh

alias e='electrum'
alias ds='electrum deserialize -'
alias sign='electrum signtransaction -'
alias start='electrum daemon -d'
alias info='electrum getinfo'
alias load='electrum load_wallet'